@ IN SOA ns.eduardocs.edu. root.eduardocs.edu. (
                                1 ; serial no
                                360000 ; refresh
                                3600 ; retry
                                3600000 ; expire
                                3600 ; default ttl
                                )
                IN NS ns.eduardocs.edu.
ns IN A 192.168.4.10
PC-01 IN A 192.168.4.11
otro.eduardocs.edu. IN A 192.168.4.12
aliaseduardocs IN CNAME ns

